{
  "label": "Entity Group",
  "id": "entitygroup",
  "supportCustomFields": true,
  "searchOnly": false,
  "supportDeletedRecordSearch": false,
  "fields": [
    {
      "id": "comments",
      "type": "textarea",
      "nlapiSubmitField": false,
      "label": "Comments",
      "required": false,
      "help": "Enter any notes or comments about the group here."
    },
    {
      "id": "email",
      "type": "email",
      "nlapiSubmitField": false,
      "label": "Email",
      "required": false,
      "help": "If this group has an address in your e-mail application, enter that address here.\n\nClicking the address from the saved record opens your e-mail program to send a message to this group."
    },
    {
      "id": "externalid",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "ExternalId",
      "required": false,
      "help": ""
    },
    {
      "id": "groupname",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "Name",
      "required": true,
      "help": "Enter the name of this group."
    },
    {
      "id": "groupowner",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Owner",
      "required": true,
      "help": "Select the owner of the group. You are selected as the owner by default. To allow only the owner to edit the group, check the Restrict Group Editing to Owner box."
    },
    {
      "id": "grouptypename",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "Type",
      "required": false,
      "help": ""
    },
    {
      "id": "isfunctionalteam",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Functional Team",
      "required": false,
      "help": "Check this box to mark this group as a functional team.\n\nFunctional teams can can be emailed copies of the issue from the Communication subtab on issue records. You may want to check this box for all product team groups and then create additional function team groups for other teams you want to email."
    },
    {
      "id": "isinactive",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Inactive",
      "required": false,
      "help": "When you check this box, you can no longer view this group's shared calendar and invite this group to events.\n\nThis group also no longer appears in the Groups list unless you check the Show Inactives box at the bottom of the page."
    },
    {
      "id": "ismanufacturingworkcenter",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Manufacturing Work Center",
      "required": false,
      "help": "Check the Manufacturing Work Center box to enable this group to be used as a work center with routing records."
    },
    {
      "id": "isprivate",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Private",
      "required": false,
      "help": "Check this box if you want only members of this group to be able to see the group's list of members."
    },
    {
      "id": "isproductteam",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Product Team",
      "required": false,
      "help": "Check this box to mark this group as a product team.\n\nProduct team groups can be selected in the Product Team field on issues. Product teams designate who is responsible for resolving an issue. Members of this team are emailed when an issue is first entered so that everyone is aware of the problem."
    },
    {
      "id": "issalesrep",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Sales Group",
      "required": false,
      "help": "Check this box to mark this group a sales group.\n\nOn sales orders and customer records, sales groups are listed in the Sales Rep field. When you assign a sales territory, sales groups are listed in the Sales Rep. field."
    },
    {
      "id": "issuerole",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Issue Role",
      "required": false,
      "help": "Select a role that corresponds to the role of this group&#x2019;s members, and this group can be assigned to issues."
    },
    {
      "id": "issupportrep",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Support Group",
      "required": false,
      "help": "Check this box to mark this group a support group.\n\nOn case records, support groups are listed in the Assigned To field. When you assign a case territory, support groups are listed in the Support Rep. field."
    },
    {
      "id": "laborresources",
      "type": "integer",
      "nlapiSubmitField": false,
      "label": "Labor Resources",
      "required": false,
      "help": "Enter the number of labor resources associated with this work center."
    },
    {
      "id": "machineresources",
      "type": "integer",
      "nlapiSubmitField": false,
      "label": "Machine Resources",
      "required": false,
      "help": "Enter the number of machine resources associated with this work center."
    },
    {
      "id": "restrictiongroup",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Restrict To",
      "required": false,
      "help": "If you want to restrict the use of this group to members of a specific group, select that group from the drop down list.\n\nIf you restrict the use of this group, then this group cannot be made private."
    },
    {
      "id": "savedsearch",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Saved Search",
      "required": true,
      "help": "Select the saved search that returns the members of this group.\n\nYou can create new saved searches at Lists > Saved Searches > New."
    },
    {
      "id": "subsidiary",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Subsidiary",
      "required": true,
      "help": ""
    },
    {
      "id": "workcalendar",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Work Calendar",
      "required": false,
      "help": "Select a work calendar to associate with this work center."
    }
  ],
