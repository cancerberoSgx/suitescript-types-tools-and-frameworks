      "required": false,
      "help": ""
    },
    {
      "id": "shipmentcreateddate",
      "type": "date",
      "nlapiSubmitField": true,
      "label": "Date Created",
      "required": false,
      "help": ""
    },
    {
      "id": "shipmentmemo",
      "type": "textarea",
      "nlapiSubmitField": true,
      "label": "Memo",
      "required": false,
      "help": "Use this field to highlight key, or frequently used information.\n\nIt is best practice to add a User Note in the Communications subtab to log call other types of communication relating to the shipment."
    },
    {
      "id": "shipmentnumber",
      "type": "text",
      "nlapiSubmitField": true,
      "label": "Shipment Number",
      "required": false,
      "help": "This is a system generated number. The format can be customized by updating the Inbound Shipment field at: \n\nSetup > Company > Auto-Generated Numbers > Transaction Numbers."
    },
    {
      "id": "shipmentstatus",
      "type": "select",
      "nlapiSubmitField": true,
      "label": "Status",
      "required": false,
      "help": "This is a read only field and updates to reflect the progress of the shipment. The five statuses are:\n    * To be Shipped\n    * In Transit\n    * Partially Received\n    * Received\n    * Closed"
    },
    {
      "id": "vesselnumber",
      "type": "text",
      "nlapiSubmitField": true,
      "label": "Vessel Number",
      "required": false,
      "help": "Input the vessel's IMO number, or other identifying reference in this field."
    }
  ],
