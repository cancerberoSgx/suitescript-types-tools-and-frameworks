    {
      "id": "eligibleforcommission",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Eligible for Commission",
      "required": false,
      "help": "Check this box if you want this partner to be able to earn commission or royalties. Partners must be assigned to a partner commission plan before they can earn commission."
