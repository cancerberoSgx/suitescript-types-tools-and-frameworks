{
  "label": "Solution",
  "id": "solution",
  "supportCustomFields": true,
  "searchOnly": false,
  "supportDeletedRecordSearch": false,
  "fields": [
    {
      "id": "assigned",
      "type": "select",
      "nlapiSubmitField": true,
      "label": "Assigned To",
      "required": false,
      "help": "If you want to assign this solution to someone, select that person in this field."
    },
    {
      "id": "autoname",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Auto",
      "required": false,
      "help": ""
    },
    {
      "id": "createddate",
      "type": "datetime",
      "nlapiSubmitField": false,
      "label": "Creation Date/Time",
      "required": false,
      "help": "The date this solution was created is shown here."
    },
    {
      "id": "customform",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Custom Form",
      "required": false,
      "help": "This field shows your default solution form.\n\nIf you select another form, this page will change to that form.\n\nTo create a custom solution form, select New."
    },
    {
      "id": "displayonline",
      "type": "checkbox",
      "nlapiSubmitField": true,
      "label": "Display Online",
      "required": false,
      "help": "Check this box if you want this solution available for published knowledge bases.\n\nIf you do not check this box, this solution will not display if you publish its topic in a knowledge base."
    },
    {
      "id": "eventnumber",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": ""
    },
    {
      "id": "externalid",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "ExternalId",
      "required": false,
      "help": ""
    },
    {
      "id": "isinactive",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Inactive",
      "required": false,
      "help": "Check this box if you no longer want to use this record.\n\nInactivating records removes them from lists and references everywhere in your account, but they are still available by clicking the Show Inactives box on lists."
    },
    {
      "id": "lastmodifieddate",
      "type": "datetime",
      "nlapiSubmitField": false,
      "label": "Last Modified Date/Time",
      "required": false,
      "help": "The date and time this solution was last modified is shown here."
    },
    {
      "id": "longdescription",
      "type": "textarea",
      "nlapiSubmitField": false,
      "label": "Description",
      "required": true,
      "help": "Enter the text of this solution or topic here.\n\nYou can use Javascript, HTML and CRMDSK tags in this description.\n\nClick Preview to see how this solution will appear when published."
    },
    {
      "id": "message",
      "type": "textarea",
      "nlapiSubmitField": true,
      "label": "Abstract",
      "required": true,
      "help": "Write an abstract or brief description about what this solution resolves. Enter text only, not HTML or other code.\n\nThis abstract shows on the Solutions list and on the Solutions subtab of case and topic records.\n\nThe abstract also shows with solution titles in published knowledge bases."
    },
    {
      "id": "solutioncode",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "Code",
      "required": false,
      "help": "To avoid duplicate solution codes, NetSuite enters the next available solution code here.\n\nYou can accept this code or enter a new code for this solution. Codes can include letters and numbers."
    },
    {
      "id": "status",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Status",
      "required": true,
      "help": "Select the status of this solution.\n\nUnapproved solutions cannot be published."
    },
    {
      "id": "title",
      "type": "text",
      "nlapiSubmitField": true,
      "label": "Title",
      "required": true,
      "help": "Enter a title for this solution.\n\nBecause solution titles can be searched, this title should be relative to the solution subject.\n\nThis title shows on the Solutions subtab of the case and topic records that this solution is applied to and in published knowledge bases."
    }
  ],
