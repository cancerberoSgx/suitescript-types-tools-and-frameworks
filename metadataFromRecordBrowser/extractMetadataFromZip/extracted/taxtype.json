{
  "label": "Tax Type",
  "id": "taxtype",
  "supportCustomFields": true,
  "searchOnly": false,
  "supportDeletedRecordSearch": false,
  "fields": [
    {
      "id": "country",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": "Select the country that you are making this record for."
    },
    {
      "id": "description",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "Description",
      "required": false,
      "help": "Enter a description for this record."
    },
    {
      "id": "doesnotaddtototal",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Does Not Add to Transaction Total",
      "required": false,
      "help": "Check this box for special tax types that do not impact the transaction total, such as withholding tax. Tax types with this property can be used on sales and purchase transactions.\n\nThis field is available only if the SuiteTax feature is enabled in your account."
    },
    {
      "id": "externalid",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "ExternalId",
      "required": false,
      "help": ""
    },
    {
      "id": "isinactive",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Inactive",
      "required": false,
      "help": "This field is false by default. Check this box to inactivate this record. Inactive records do not show on transactions and records for selection in lists.\n\nOn nexus and tax type records, this field is available only if the SuiteTax feature is enabled in your account."
    },
    {
      "id": "name",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "Name",
      "required": true,
      "help": "Enter a name for this record.\n\nOn tax code records, this field is available only if the SuiteTax feature is enabled in your account."
    },
    {
      "id": "posttoitemcost",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Post to Item Cost",
      "required": false,
      "help": "Check this box if the tax amounts for the tax type should be added to the related item cost. Tax types with this property can be used on purchase transactions only.\n\nThis field is available only if the SuiteTax feature is enabled in your account."
    },
    {
      "id": "reversecharge",
      "type": "checkbox",
      "nlapiSubmitField": true,
      "label": "Reverse Charge",
      "required": false,
      "help": "Check this box if reverse charge applies to transactions that have this tax type. Tax types with this property can be used on purchase transactions only.\n\nOn tax code records, this field is available only if the SuiteTax feature is disabled in your account.\n\nOn tax type records, this field is available only if the SuiteTax feature is enabled in your account."
    },
    {
      "id": "taxinnetamount",
      "type": "checkbox",
      "nlapiSubmitField": true,
      "label": "Tax Included in Net Amount",
      "required": false,
      "help": "Check this box for special tax types where the tax amount is included in the item price. Tax types with this property can be used on sales and purchase transactions.\n \nThis field is available only if the SuiteTax feature is enabled in your account."
    }
  ],
