{
  "label": "Phone Call",
  "id": "phonecall",
  "supportCustomFields": true,
  "searchOnly": false,
  "supportDeletedRecordSearch": false,
  "fields": [
    {
      "id": "accesslevel",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Private Phone Call",
      "required": false,
      "help": "Check this box to have this call show only in your Phone Calls list.\n\nCalls that are not private can be seen by others with access to your account."
    },
    {
      "id": "assigned",
      "type": "select",
      "nlapiSubmitField": true,
      "label": "Organizer",
      "required": true,
      "help": "Select the employee responsible for setting up or making this phone call."
    },
    {
      "id": "company",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Company",
      "required": false,
      "help": "Select the customer, vendor or partner this call pertains to.\n\nSelecting a company adds this call to the associated record.\n\nYou can also select this company on the Participants subtab."
    },
    {
      "id": "completeddate",
      "type": "date",
      "nlapiSubmitField": false,
      "label": "Date Completed",
      "required": false,
      "help": "Enter the date the subject for this call was concluded.\n\nThis field can be useful if the call required a follow-up or call back or was not completed on the call date for another reason."
    },
    {
      "id": "contact",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Contact",
      "required": false,
      "help": "Select the contact for this company in relation to this cal.\n\nThe contacts in this field are the contacts attached to the company selected above. Click Open next to the Company field to add contacts to a company's record."
    },
    {
      "id": "createddate",
      "type": "datetime",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": ""
    },
    {
      "id": "customform",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Custom Form",
      "required": true,
      "help": "If you have created a custom entry form for phone call records, you can select that form here. Selecting the form changes the page to your custom record.\n\nAn administrator can create custom records at <_TABNAME=LIST_CUSTENTRYFORM_> > <_TASKCATEGORY=LIST_CUSTENTRYFORM_> > Custom Entry Forms."
    },
    {
      "id": "enddate",
      "type": "text",
      "nlapiSubmitField": true,
      "label": "",
      "required": false,
      "help": "Enter the date this call must be made by, or the deadline."
    },
    {
      "id": "endtime",
      "type": "timeofday",
      "nlapiSubmitField": false,
      "label": "End Time",
      "required": true,
      "help": ""
    },
    {
      "id": "endtimepicker",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": ""
    },
    {
      "id": "externalid",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "ExternalId",
      "required": false,
      "help": ""
    },
    {
      "id": "group",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": ""
    },
    {
      "id": "lastmodifieddate",
      "type": "datetime",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": ""
    },
    {
      "id": "message",
      "type": "textarea",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": "Enter notes on the context and decisions made in this phone call.\n\nYou can enter up to 999 characters."
    },
    {
      "id": "owner",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "Created By",
      "required": true,
      "help": ""
    },
    {
      "id": "phone",
      "type": "phone",
      "nlapiSubmitField": true,
      "label": "Phone Number",
      "required": false,
      "help": "Enter the phone number used for this call.\n\nPhone numbers can be entered in the following formats: 999-999-9999, 1-999-999-9999, (999) 999-9999, 1(999) 999-9999 or 999-999-9999 ext 9999."
    },
    {
      "id": "priority",
      "type": "select",
      "nlapiSubmitField": true,
      "label": "Priority",
      "required": true,
      "help": ""
    },
    {
      "id": "reminderminutes",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Reminder",
      "required": false,
      "help": "Select the amount of time before the start time for this call when you would like to set a reminder for the organizer.\n\nYou must check the Block Out Time box to set a reminder.\n\nYou must also select a reminder type to set the reminder."
    },
    {
      "id": "remindertype",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Reminder Type",
      "required": false,
      "help": "Select the type of reminder the call organizer should receive before the start time of this task.\n\nYou must check the Block Out Time box to set a reminder.\n\nEmail reminders are sent to the email address on the employee record of the organizer.\n\nPopup reminders appear only if the organizer is logged in to his or her NetSuite account.\n\nYou must also set a time in the Reminder field to set a reminder."
    },
    {
      "id": "sendemail",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Send Email",
      "required": false,
      "help": ""
    },
    {
      "id": "startdate",
      "type": "date",
      "nlapiSubmitField": true,
      "label": "Date",
      "required": true,
      "help": "Enter the date of this call.\n\nClick the calendar icon to choose the date from the calendar."
    },
    {
      "id": "starttime",
      "type": "timeofday",
      "nlapiSubmitField": false,
      "label": "Start Time",
      "required": true,
      "help": ""
    },
    {
      "id": "starttimepicker",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": ""
    },
    {
      "id": "status",
      "type": "select",
      "nlapiSubmitField": true,
      "label": "Status",
      "required": true,
      "help": "Select either Scheduled or Completed as the status of this call.\n\nCompleted calls no longer show on your Phone Calls list by default."
    },
    {
      "id": "supportcase",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Support Case",
      "required": false,
      "help": "If this call is related to a support case, select the case number here.\n\nSelecting a case attaches this phone call record to the case record."
    },
    {
      "id": "timedevent",
      "type": "checkbox",
      "nlapiSubmitField": false,
      "label": "Reserve Time",
      "required": false,
      "help": "Check this box to schedule this call on the calendar of the person organizing the call.\n\nEnter the start and end times in the From and To fields.\n\nThis time is scheduled on the calendar for the phone call date."
    },
    {
      "id": "timezone",
      "type": "text",
      "nlapiSubmitField": false,
      "label": "",
      "required": false,
      "help": ""
    },
    {
      "id": "title",
      "type": "text",
      "nlapiSubmitField": true,
      "label": "Subject",
      "required": true,
      "help": "Enter the subject of this phone call.\n\nYou can enter up to 100 characters."
    },
    {
      "id": "transaction",
      "type": "select",
      "nlapiSubmitField": false,
      "label": "Transaction",
      "required": false,
      "help": "If this call is associated with a transaction, select that transaction here.\n\nIf you select a transaction, this phone call is listed on the History subtab of the transaction."
    }
  ],
