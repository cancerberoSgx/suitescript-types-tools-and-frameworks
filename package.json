        "build-watch": "npx tsc -w",
        "dev": "npx gulp",
        "all": "npm run clean && npm run build && npm test && npm run test-js"
    },
    "keywords": [
        "SuiteScript",
        "SuiteScript 2.0",
        "NetSuite",
        "utilities",
        "high level APIs",
        "API",
        "testing",
        "Commerce",
        "TypeScript"
    ],
    "author": "Sebasti√°n Gurin",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@hitc/netsuite-types": "*",
        "@types/shelljs": "^0.8.1",
        "gulp": "^3.9.1",
        "gulp-watch": "^4.3.11",
        "netsuite-uploader-util": "^1.1.3",
        "shelljs": "^0.7.8",
        "ts-node": "^7.0.1",
        "typescript": "^2.6.2"
    }
