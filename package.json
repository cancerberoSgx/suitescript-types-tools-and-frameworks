{
    "name": "sgurin-suitescript-utils",
    "version": "0.0.2",
    "description": " Miscellaneous SuiteScript 2.0 high level utilities and APIs (mostly oriented to Commerce)",
    "main": "src/index.js",
    "private": true,
    "scripts": {
        "test": "cd spec && npm run run",
        "test-js": "cd spec && npm run run-js",
